package com.api.agviagem.models;

import javax.persistence.Column;

public class EnderecoPessoa {
	
	@Column(nullable = false)
	private String numero;
	
	@Column(nullable = false)
	private String rua;
	
	@Column(nullable = false)
	private String bairro;
	
	@Column(nullable = false)
	private String cidade;
}
